{
  "name": "kris-restaurant-agent",
  "version": "1.0.0",
  "description": "Restaurant booking agent.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "npm run clean && tsc",
    "build:rollup": "npm run clean && rollup -c",
    "build:webpack": "npm run clean && webpack --mode production",
    "build:vite": "npm run clean && vite build",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist coverage .nyc_output .tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src --ext .ts,.js --fix",
    "lint:check": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:ci",
    "prepack": "npm run validate",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "schema": "curl -s http://localhost:3000/schema | jq '.' || echo 'Server not running or jq not installed'",
    "deploy": "echo \"Deployment not configured yet. See README.md for deployment options.\"",
    "docker:build": "echo \"Docker not configured for this project\"",
    "docker:run": "echo \"Docker not configured for this project\"",
    "update-deps": "npx npm-check-updates -u && npm install",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "ai-spine",
    "tool",
    "kris-restaurant-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-spine/tools": "^1.1.2",
    "@ai-spine/tools-testing": "^1.1.2",
    "ai-spine-sdk": "^2.5.4",
    "axios": "^1.12.0",
    "dotenv": "^17.2.2",
    "npm-check": "^3.2.10",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "superagent": "^10.2.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
